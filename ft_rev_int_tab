#include <stdio.h>
int main() 
{
    int size=3;
    int array[]={2,3,5};
    int aux_array[size];
    int counter;  
    
    int *array_pointer[size]; // creamos puntero 
    array_pointer[size]=&array[size];
    *array_pointer[size]=array[size]; // apuntamos a la array
    
    // priint original array
    printf("our array is: \n");
    while (counter < size)
    {
        printf("%d\n", array[counter]);
        counter ++;
    }
    
    // priint original array_pointer
    printf("our ORIGINAL array_pointer is: \n");
    while (counter < size)
    {
        printf("%d\n", *array_pointer[counter]);
        counter ++;
    }
    printf("\n");
    
     // priint original array_pointer
    printf("our INVERTED array_pointer is: \n");
    while (counter < size)
    {
        printf("%d\n", *array_pointer[counter]);
        counter ++;
    }
    
    return 0;
}
